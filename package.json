{
  "name": "svelte-kitui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "docs:dev": "turbo run dev --filter=docs --parallel",
    "docs:build": "turbo run build --filter=docs...",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.30.0"
}
